#!groovy
//noinspection GroovyUnusedAssignment
@Library("Infrastructure@feature/ROC-3460-One-app-insights-per-product") _

import uk.gov.hmcts.contino.Environment

Environment environment = new Environment(env)

onMaster {
  sharedInfrastructurePipeline('cmc', environment.nonProdName, 'prod')
  sharedInfrastructurePipeline('cmc', environment.prodName, 'prod')
}

onDemo {
  sharedInfrastructurePipeline('cmc', environment.demoName, 'prod')
}
