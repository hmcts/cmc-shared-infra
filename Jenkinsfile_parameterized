#!groovy
//noinspection GroovyUnusedAssignment
@Library("Infrastructure") _

//noinspection GroovyAssignabilityCheck
properties([
		parameters([
			choice(name: 'ENVIRONMENT', choices: 'sandbox\nsaat\nsprod', description: 'Environment where code should be build and deployed')
		])
])

withParameterizedInfraPipeline('cmc', params.ENVIRONMENT, 'sandbox') {
    enableSlackNotifications('#cmc-tech-notification')
}